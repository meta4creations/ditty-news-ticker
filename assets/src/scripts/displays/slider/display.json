{
  "type": "slider",
  "label": "Slider",
  "description": "Display items in a slider.",
  "icon": "fa-arrows-left-right",
  "version": "1.0.0",
  "textdomain": "ditty-news-ticker",
  "defaults": {
    "slidesPerView": "1",
    "slidesSpacing": "0",
    "autoplay": "0",
