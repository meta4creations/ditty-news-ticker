      "paddingTop": "",
      "paddingBottom": "",
      "paddingLeft": "",
      "paddingRight": ""
    },
    "borderStyle": "none",
    "contentsBorderStyle": "none",
    "pageBorderStyle": "none",
    "itemBorderStyle": "none",
    "titleDisplay": "none",
    "titleContentsSize": "stretch",
