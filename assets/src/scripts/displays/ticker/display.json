{
  "type": "ticker",
  "label": "Ticker",
  "description": "Basic news ticker display.",
  "icon": "fas fa-ellipsis-h",
  "version": "1.0.0",
  "textdomain": "ditty-news-ticker",
  "defaults": {
    "direction": "left",
    "itemMaxWidth": "300px",
    "itemElementsWrap": "nowrap",
    "minHeight": "300px",
    "spacing": "25",
    "speed": "10",
    "heightEase": "easeInOutQuint",
    "heightSpeed": "1.5",
    "scrollInit": "empty",
    "scrollDelay": "3",
    "cloneItems": "yes",
    "wrapItems": "yes",
    "hoverPause": "",
    "titleDisplay": "none",
    "titleContentsSize": "stretch",
    "titleContentsPosition": "start",
    "titleElement": "h3",
    "titleElementPosition": "start",
    "titleElementVerticalPosition": "start"
  },
  "editorScript": [
    "file:./editor.js"
  ],
  "displayScript": [
    "file:./display.js"
  ],
  "style": [
    "file:./display.css"
  ]
}